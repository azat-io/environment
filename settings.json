{
  "workbench.iconTheme": "gruvbox-icon-theme",
  "editor.fontFamily": "'Martian Mono sWd Rg', Monaco, 'Courier New', monospace",
  "scm.inputFontSize": 14,
  "diffEditor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.lineHeight": 18,
  "editor.suggestLineHeight": 18,
  "terminal.integrated.fontSize": 9.5,
  "debug.console.fontSize": 9.75,
  "terminal.integrated.lineHeight": 1.65,
  "editor.bracketPairColorization.enabled": false,
  "editor.wordWrap": "on",
  "stylelint.packageManager": "pnpm",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.confirmDragAndDrop": false,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "prettier.prettierPath": "./node_modules/prettier",
  "wallaby.colors": {
    "covered": "#b8bb26",
    "errorPath": "#fabd2f",
    "errorSource": "#fb4934",
    "notCovered": "#928374",
    "partiallyCovered": "#fe8019",
    "snapshot.covered": "#b8bb26",
    "snapshot.errorPath": "#fabd2f",
    "snapshot.errorSource": "#fb4934",
    "snapshot.notCovered": "#928374",
    "snapshot.partiallyCovered": "#fe8019"
